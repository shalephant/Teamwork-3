TABLE students {
  id int [pk]
  name varchar
  surname varchar
  personalid int
  email varchar
  address varchar
  dateofbirth datetime
  gender varchar
}

TABLE extrainfo {
  id int [pk]
  secondaddress varchar
  aptnumber int
  student int
}

TABLE Professors {
  id int [pk]
  name varchar
  surname varchar
  personalid int
  email varchar
  address varchar
  dateofbirth datetime
  gender varchar
  accountnumber varchar
  level varchar
}

TABLE managers {
  id int [pk]
  name varchar
  surname varchar
  personalid int
  email varchar
  address varchar
  dateofbirth datetime
  gender varchar
}

TABLE course {
  id int [pk]
  name varchar
}

TABLE subject {
  id int [pk]
  name varchar
  course int
  Professors int
}

TABLE faculty {
  id int [pk]
  name varchar
  subject int
  student int
  managers int
}

TABLE lecture {
  id int [pk]
  time datetime
  subject int
  student int
}

Ref: extrainfo.student > students.id
Ref: subject.course > course.id
Ref: subject.Professors > Professors.id
Ref: faculty.student > students.id
Ref: faculty.subject > subject.id
Ref: faculty.managers > managers.id
Ref: lecture.subject > subject.id
Ref: lecture.student > students.id



/*   MySQL Diagram   */

CREATE Table students (
  id int NOT NULL,
  name varchar,
  surname varchar,
  personalid int,
  email varchar,
  address varchar,
  dateofbirth datetime,
  gender varchar
);

CREATE Table extrainfo  (
  id int NOT NULL,
  secondaddress varchar,
  aptnumber int,
  student int
);

CREATE Table professors (
  id int NOT NULL,
  name varchar,
  surname varchar,
  personalid int,
  email varchar,
  address varchar,
  dateofbirth datetime,
  gender varchar,
  accountnumber varchar,
  level varchar
);

CREATE Table managers (
  id int NOT NULL,
  name varchar,
  surname varchar,
  personalid int,
  email varchar,
  address varchar,
  dateofbirth datetime,
  gender varchar
);

CREATE Table course (
  id int NOT NULL,
  name varchar
);

CREATE Table subject (
  id int NOT NULL,
  name varchar,
  course int,
  professors int,
  faculty id
);

CREATE Table faculty (
  id int NOT NULL,
  name varchar,
  managers int
);

CREATE Table lecture (
  id int NOT NULL,
  lecturetime datetime,
  subject int,
  student int
);


